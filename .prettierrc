{
  "singleQuote": true,
  "printWidth": 80,
  "semi": false,
  "trailingComma": "all",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "",
    "^antd/(.*)$",
    "^@ant-design/(.*)$",
    "",
    "^@tanstack/(.*)$",
    "^@(.*)$",
    "",
    "^[./]"
  ]
}
